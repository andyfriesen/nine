# Ackermann's function.
# A completely artificial benchmark, but one that demonstrates recursion and some other fun things.

def ackermann(x as int, y as int) as int:
    if x == 0:
        return y + 1

    if y == 0:
        return ackermann(x - 1, 1)
    else:
        return ackermann(x - 1, ackermann(x, y - 1))

print ackermann(3, 4)
